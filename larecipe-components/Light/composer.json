{
    "name": "terribooks/light",
    "description": "A LaRecipe theme.",
    "keywords": [
        "laravel",
        "larecipe"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Terribooks\\Light\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Terribooks\\Light\\ThemeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
